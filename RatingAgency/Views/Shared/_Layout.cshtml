
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    
    <title>@ViewBag.Title</title>
</head>

<body>
    <div class="container">
        <header>
            <nav class="navbar navbar-expand-lg navbar-dark fixed-top bg-primary" aria-label="Navigation Header">
                <div class="container-xl">
                    <a class="navbar-brand">
                        <img src="/Images/logo.png" width="151" height="auto" alt="app log" />
                    </a>
                    <div class="pagination">
                        <ul class="breadcrumb">
                            <li>
                                <a class="" type="button" data-bs-toggle="collapse" asp-controller="Home" asp-action="Index">
                                    <span class="divider">Home</span>
                                </a>
                            </li>
                            <li>
                                <a class="" type="button" data-bs-toggle="collapse" asp-controller="Proposal" asp-action="Index">
                                    <span class="divider">Proposals</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>


        </header>
        @RenderBody()
    </div>

    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/shared/_layout.js"></script>
    <link href="~/css/site.css" rel="stylesheet"></link>
    <link href="~/css/bootstrap.css" rel="stylesheet"></link>
    <base href="/" />
</body>
</html>


    

